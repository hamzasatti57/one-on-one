<p id="notice"><%= notice %></p>

<h1>Stores</h1>

<table>
  <thead>
    <tr>
      <th>Store name</th>
      <th>Store city</th>
      <th>Store address</th>
      <th>Store city</th>
      <th>Store country</th>
      <th>Store state</th>
      <th>Store area</th>
      <th>Store map link</th>
      <th>Stroe active</th>
      <th>Store on time</th>
      <th>Store close time</th>
      <th>Vender</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stores.each do |store| %>
      <tr>
        <td><%= store.store_name %></td>
        <td><%= store.store_city %></td>
        <td><%= store.store_address %></td>
        <td><%= store.store_city %></td>
        <td><%= store.store_country %></td>
        <td><%= store.store_state %></td>
        <td><%= store.store_area %></td>
        <td><%= store.store_map_link %></td>
        <td><%= store.stroe_active %></td>
        <td><%= store.store_on_time %></td>
        <td><%= store.store_close_time %></td>
        <td><%= store.vender %></td>
        <td><%= link_to 'Show', store %></td>
        <td><%= link_to 'Edit', edit_store_path(store) %></td>
        <td><%= link_to 'Destroy', store, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Store', new_store_path %>
