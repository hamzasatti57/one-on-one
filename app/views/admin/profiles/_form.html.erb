<%= render 'shared/errors', obj: profile %>
<%= form_for  profile, profile_form_attributes(profile) do |f| %>
<!--  <div class="form-group">-->
    <%#= f.label :DateofBirth %>
<!--    <%#= f.date_field :dob, class: "form-control", autocomplete: false, placeholder: "DateofBirth", autofocus: true %> <br/>-->
<!--  </div>-->
<!--  <div class="form-group">-->
<!--    <%#= f.label :gender %><br>-->
    <%#= f.radio_button(:gender, "male") %>
    <%#= label_tag("Male") %>
    <%#= f.radio_button(:gender, "female") %>
    <%#= label_tag("Female") %>
<!--    <%#= f.gender :dob, class: "form-control", autocomplete: false, placeholder: "DateofBirth", autofocus: true %> <br/>-->
<!--  </div>-->
  <div class="form-group">
    <%= f.label :contact %>
    <%= f.text_field :phone, class: "form-control", autocomplete: false, placeholder: "Contact Number", autofocus: true %> <br/>
  </div>
  <div class="form-group">
    <%= f.label :address %>
    <%= f.text_field :address, class: "form-control", autocomplete: false, placeholder: "Contact Address", autofocus: true %> <br/>
  </div>
  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_area :description, class: "form-control", autocomplete: false, placeholder: "Description", autofocus: true %> <br/>
  </div>
  <div class="form-group">
    <%= f.label :facebook %>
    <%= f.text_field :facebook, class: "form-control", autocomplete: false, placeholder: "facebook", autofocus: true %> <br/>
  </div>
  <div class="form-group">
    <%= f.label :instagram %>
    <%= f.text_field :instagram, class: "form-control", autocomplete: false, placeholder: "instagram", autofocus: true %> <br/>
  </div>
  <div class="form-group">
    <%= f.label :twitter %>
    <%= f.text_field :twitter, class: "form-control", autocomplete: false, placeholder: "Twitter", autofocus: true %> <br/>
  </div>
  <%= f.label :Catelog  %>
  <%= f.file_field :pdf%><br><br>
  <%= f.label :Profile_Picture  %>
  <%= f.file_field :image%><br><br>
  <%= f.submit class: "btn btn-block btn-primary custom-table-edit-btn"%>
<% end %>