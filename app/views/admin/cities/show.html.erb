<div class="row">
  <div class="col-lg-9 mt-3 offset-2">
    <div class="card">
      <div class="card-header custom-card-header">
        <strong>City</strong>
      </div>
      <div class="card-body">
        <div class="well col-xs-8 col-xs-offset-2">
          <h4>City Name: </h4><br>
          <p><%= @city.title%></p>
          <div class="Center">
            <% if @city.image.attached? %>
              <%= image_tag(url_for(@city.image.variant(resize: '400x400'))) %>
            <% end %>
          </div>
          <div class="article-actions">
            <%= link_to "Edit",  edit_admin_city_path(@city), class: "btn btn-xs custom-table-edit-btn btn-primary" %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
