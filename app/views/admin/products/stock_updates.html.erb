<div class="card">
  <div class="card-header custom-card-header">
    <i class="fa fa-align-justify"></i> Stock Update</div>
  <div class="card-body">
    <div class="row">
      <div class="col-lg-12">
        <br>
        <p>Stock Update:<span>  <%= @stock_update.stock_file.strftime("%b %d, %Y %I:%M %P") if @stock_update.present? && @stock_update.stock_file.present? %></span></p>
        <p>Inventory Update:<span>  <%= @stock_update.gp_stock_file.strftime("%b %d, %Y %I:%M %P") if @stock_update.present? && @stock_update.gp_stock_file.present? %></span></p>  
      </div>
    </div>
  </div>
</div>